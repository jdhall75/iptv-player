{
  "name": "iptv-player-backend",
  "version": "1.0.0",
  "description": "IPTV Player Backend - Cloudflare Workers API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create iptv-player-db",
    "db:migrate": "wrangler d1 migrations apply iptv-player-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply iptv-player-db --remote",
    "db:console": "wrangler d1 execute iptv-player-db --local --command",
    "test": "vitest",
    "test:api": "./test-api.sh"
  },
  "keywords": [
    "iptv",
    "cloudflare",
    "workers"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "wrangler": "^4.45.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "fast-xml-parser": "^5.3.3",
    "hono": "^4.0.0",
    "jose": "^5.0.0"
  }
}
